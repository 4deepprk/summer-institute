@article{lomborg_personal_2013,
	title = {Personal internet archives and ethics},
	volume = {9},
	issn = {1747-0161, 2047-6094},
	url = {http://rea.sagepub.com/content/9/1/20},
	doi = {10.1177/1747016112459450},
	abstract = {In its ethics guidelines, the Association of Internet Researchers advocates a bottom-up, case-based approach to research ethics, one that emphasizes that ethical judgement must be based on a sensible examination of the unique object and circumstances of a study, its research questions, the data involved, and the expected analysis and reporting of results, along with the possible ethical dilemmas arising from the case. This article clarifies and illustrates the mind-set and process of such a bottom-up approach to internet research ethics. Two ethics concepts to think with, namely {\textquoteleft}the distance principle{\textquoteright}, and the notion of {\textquoteleft}perceived privacy{\textquoteright}, are introduced and applied in a concrete empirical internet study, in which web archives based on personal communications on social media formed the main body of data. The empirical case example serves to highlight the unique challenges of internet research ethics, in light of the blurring boundaries between text and person, and between private and public on the internet, with profound implications and challenges for the definition of human subjects, privacy, and so on.},
	language = {en},
	number = {1},
	urldate = {2015-09-15},
	journal = {Research Ethics},
	author = {Lomborg, Stine},
	month = mar,
	year = {2013},
	keywords = {internet research ethics, perceived privacy, social media, the distance principle, web archives},
	pages = {20--31},
	file = {Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/8JR5VJX4/Lomborg - 2013 - Personal internet archives and ethics.pdf:application/pdf;Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/QMMPEV9B/20.html:text/html}
}

@article{kosinski_facebook_2015,
	title = {Facebook as a research tool for the social sciences: {Opportunities}, challenges, ethical considerations, and practical guidelines},
	volume = {70},
	copyright = {(c) 2015 APA, all rights reserved},
	issn = {1935-990X(Electronic);0003-066X(Print)},
	shorttitle = {Facebook as a research tool for the social sciences},
	doi = {10.1037/a0039210},
	abstract = {Facebook is rapidly gaining recognition as a powerful research tool for the social sciences. It constitutes a large and diverse pool of participants, who can be selectively recruited for both online and offline studies. Additionally, it facilitates data collection by storing detailed records of its users{\textquoteright} demographic profiles, social interactions, and behaviors. With participants{\textquoteright} consent, these data can be recorded retrospectively in a convenient, accurate, and inexpensive way. Based on our experience in designing, implementing, and maintaining multiple Facebook-based psychological studies that attracted over 10 million participants, we demonstrate how to recruit participants using Facebook, incentivize them effectively, and maximize their engagement. We also outline the most important opportunities and challenges associated with using Facebook for research, provide several practical guidelines on how to successfully implement studies on Facebook, and finally, discuss ethical considerations.},
	number = {6},
	journal = {American Psychologist},
	author = {Kosinski, Michal and Matz, Sandra C. and Gosling, Samuel D. and Popov, Vesselin and Stillwell, David},
	year = {2015},
	keywords = {*Experimental Design, *Experimental Ethics, *Sampling (Experimental), *Social Sciences, online social networks},
	pages = {543--556}
}

