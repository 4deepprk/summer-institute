@article{gasson_normality_2011,
	title = {Normality {Mining}: {Privacy} {Implications} of {Behavioral} {Profiles} {Drawn} {From} {GPS} {Enabled} {Mobile} {Phones}},
	volume = {41},
	issn = {1094-6977},
	shorttitle = {Normality {Mining}},
	doi = {10.1109/TSMCC.2010.2071381},
	abstract = {There is growing interest in the ways in which the location of a person can be utilized by new applications and services. Recent advances in mobile technologies have meant that the technical capability to record and transmit location data for processing is appearing in off-the-shelf handsets. This opens possibilities to profile people based on the places they visit, people they associate with, or other aspects of their complex routines determined through persistent tracking. It is possible that services offering customized information based on the results of such behavioral profiling could become commonplace. However, it may not be immediately apparent to the user that a wealth of information about them, potentially unrelated to the service, can be revealed. Further issues occur if the user agreed, while subscribing to the service, for data to be passed to third parties where it may be used to their detriment. Here, we report in detail on a short case study tracking four people, in three European member states, persistently for six weeks using mobile handsets. The GPS locations of these people have been mined to reveal places of interest and to create simple profiles. The information drawn from the profiling activity ranges from intuitive through special cases to insightful. In this paper, these results and further extensions to the technology are considered in light of European legislation to assess the privacy implications of this emerging technology.},
	number = {2},
	journal = {IEEE Transactions on Systems, Man, and Cybernetics, Part C: Applications and Reviews},
	author = {Gasson, M.N. and Kosta, E. and Royer, D. and Meints, M. and Warwick, K.},
	month = mar,
	year = {2011},
	keywords = {behavioral profiles, behavioral profiling, complex routines, customized information, data mining, data privacy, European legislation, Global Positioning System, GPS enabled mobile phones, location data, mobile computing, mobile handsets, mobile technologies, normality mining, off-the-shelf handsets, persistent tracking, privacy implications, profiling activity, technical capability},
