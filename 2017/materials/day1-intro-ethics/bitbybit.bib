	journal = {SAGE Open},
	author = {Hauser, David J. and Schwarz, Norbert},
	month = apr,
	year = {2015},
	pages = {2158244015584617},
	file = {Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/SKGDT5EI/Hauser and Schwarz - 2015 - It{\textquoteright}s a Trap! Instructional Manipulation Checks Pro.pdf:application/pdf;Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/3RF3Z5EZ/2158244015584617.html:text/html}
}

@article{hauser_attentive_2015,
