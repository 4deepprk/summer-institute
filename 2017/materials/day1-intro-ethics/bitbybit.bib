	pages = {e890},
	file = {PLoS Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/TA2C5SSS/Beatty et al. - 2010 - Best Practices in Dengue Surveillance A Report fr.pdf:application/pdf;PLoS Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/CMJEXU7N/infodoi10.1371journal.pntd.html:text/html}
}

