@article{corrigan_empty_2003,
	title = {Empty ethics: the problem with informed consent},
	volume = {25},
	issn = {1467-9566},
	shorttitle = {Empty ethics},
	url = {http://onlinelibrary.wiley.com/doi/10.1046/j.1467-9566.2003.00369.x/abstract},
	doi = {10.1046/j.1467-9566.2003.00369.x},
	abstract = {Abstract  Informed consent is increasingly heralded as an ethical panacea, a tool to counter autocratic and paternalistic medical practices. Debate about the implementation of informed consent is constricted and polarised, centring on the right of individuals to be fully informed and to freely choose versus an autocratic, paternalistic practice that negates individual choice. A bioethical framework, based on a principle-led form of reductive/deductive reasoning, dominates the current model of informed consent. Such a model tends to abstract the process of consent from its clinical and social setting. By fleshing out the social process involved when patients and healthy volunteer subjects consent to take part in clinical drug trials, this paper attempts to address the problem arising from the current {\textquoteleft}empty ethics{\textquoteright} model. My arguments are substantiated by qualitative interview data drawn from a study I conducted on the process of consent as experienced by participants in clinical drug trials.},
	language = {en},
	number = {7},
	urldate = {2015-09-18},
	journal = {Sociology of Health \& Illness},
	author = {Corrigan, Oonagh},
	month = nov,
	year = {2003},
	keywords = {autonomy, clinical drug trials, ethics, Informed consent, paternalism},
	pages = {768--792},
	file = {Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/6NWDMNHA/Corrigan - 2003 - Empty ethics the problem with informed consent.pdf:application/pdf;Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/4HU7SAZB/full.html:text/html}
}

