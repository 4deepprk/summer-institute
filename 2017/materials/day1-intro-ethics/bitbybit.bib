	pages = {e56515},
	file = {PLoS Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/9II2VXNV/Shanks et al. - 2013 - Priming Intelligent Behavior An Elusive Phenomeno.pdf:application/pdf;PLoS Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/WJN5K4RV/infodoi10.1371journal.pone.html:text/html}
}

