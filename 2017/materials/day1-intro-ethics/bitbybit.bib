	year = {2015},
	pmid = {25063043},
	keywords = {causal inference, confounder bias, Mediation},
	pages = {30--43},
	file = {Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/E4A53PD4/MacKinnon and Pirlott - 2015 - Statistical Approaches for Enhancing Causal Interp.pdf:application/pdf;Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/GX9GVEZ8/30.html:text/html}
}

