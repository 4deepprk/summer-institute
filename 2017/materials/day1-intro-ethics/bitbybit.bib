@article{wang_cooperation_2012,
	title = {Cooperation and assortativity with dynamic partner updating},
	issn = {0027-8424, 1091-6490},
	url = {http://www.pnas.org/content/early/2012/08/16/1120867109},
	doi = {10.1073/pnas.1120867109},
	abstract = {The natural tendency for humans to make and break relationships is thought to facilitate the emergence of cooperation. In particular, allowing conditional cooperators to choose with whom they interact is believed to reinforce the rewards accruing to mutual cooperation while simultaneously excluding defectors. Here we report on a series of human subjects experiments in which groups of 24 participants played an iterated prisoner{\textquoteright}s dilemma game where, critically, they were also allowed to propose and delete links to players of their own choosing at some variable rate. Over a wide variety of parameter settings and initial conditions, we found that dynamic partner updating significantly increased the level of cooperation, the average payoffs to players, and the assortativity between cooperators. Even relatively slow update rates were sufficient to produce large effects, while subsequent increases to the update rate had progressively smaller, but still positive, effects. For standard prisoner{\textquoteright}s dilemma payoffs, we also found that assortativity resulted predominantly from cooperators avoiding defectors, not by severing ties with defecting partners, and that cooperation correspondingly suffered. Finally, by modifying the payoffs to satisfy two novel conditions, we found that cooperators did punish defectors by severing ties, leading to higher levels of cooperation that persisted for longer.},
	language = {en},
	urldate = {2012-09-11},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Wang, Jing and Suri, Siddharth and Watts, Duncan J.},
	month = aug,
	year = {2012},
	keywords = {dynamic networks, network science, web-based experiments},
	file = {Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/43FR56S6/Wang et al. - 2012 - Cooperation and assortativity with dynamic partner.pdf:application/pdf;Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/HQ98F457/1120867109.html:text/html}
}

@article{horton_employer_2010,
	title = {Employer {Expectations}, {Peer} {Effects} and {Productivity}: {Evidence} from a {Series} of {Field} {Experiments}},
	shorttitle = {Employer {Expectations}, {Peer} {Effects} and {Productivity}},
	url = {http://papers.ssrn.com/sol3/papers.cfm?abstract_id=1652993},
	abstract = {reports the results of a series of field experiments designed to investigate how peer effects operate in a real work setting. Workers were hired from an online labor market to perform an image-labeling task and, in some cases, to evaluate the work product of other workers. These evaluations had financial consequences for both the evaluator and the evaluated. I find that, on average, evaluating high-output work raised an evaluator's subsequent productivity, with larger effects for evaluators that are themselves highly productive. The content of the subject evaluations themselves suggest one mechanism for peer effects: workers readily punished other workers whose work product exhibited low output/effort. However, non-compliance with employer expectations did not, by itself, trigger punishment: workers would not punish non-complying workers so long as the evaluated worker still exhibited high effort. A worker's willingness to punish was strongly correlated with their own productivity, yet this relationship was not the result of innate differences {\textendash} productivity-reducing manipulations also resulted in reduced punishment. Peer effects proved hard to stamp out: although most workers complied with clearly communicated maximum expectations for output, some workers still raised their production beyond the output ceiling after evaluating highly productive yet non-complying work products.},
	urldate = {2011-05-04},
	journal = {SSRN eLibrary},
	author = {Horton, John J.},
	month = aug,
	year = {2010},
	keywords = {Field Experiments, Mechanical Turk, Peer Effects, Productivity},
	file = {SSRN Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/STKKV7FR/papers.html:text/html}
}

