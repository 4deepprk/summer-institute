@article{williamson_ethics_2016,
	title = {On the {Ethics} of {Crowdsourced} {Research}},
	volume = {49},
	issn = {1537-5935},
	url = {http://journals.cambridge.org/article_S104909651500116X},
	doi = {10.1017/S104909651500116X},
	abstract = {ABSTRACT This article examines the ethics of crowdsourcing in social science research, with reference to my own experience using Amazon{\textquoteright}s Mechanical Turk. As these types of research tools become more common in scholarly work, we must acknowledge that many participants are not one-time respondents or even hobbyists. Many people work long hours completing surveys and other tasks for very low wages, relying on those incomes to meet their basic needs. I present my own experience of interviewing Mechanical Turk participants about their sources of income, and I offer recommendations to individual researchers, social science departments, and journal editors regarding the more ethical use of crowdsourcing.},
	number = {01},
	urldate = {2016-07-11},
	journal = {PS: Political Science \& Politics},
	author = {Williamson, Vanessa},
	month = jan,
