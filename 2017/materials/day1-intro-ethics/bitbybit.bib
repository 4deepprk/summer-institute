@book{lowrance_privacy_2012,
	title = {Privacy, {Confidentiality}, and {Health} {Research}},
	isbn = {978-1-107-02087-0},
	abstract = {The potential of the e-health revolution, increased data sharing, database linking, biobanks and new techniques such as geolocation and genomics to advance human health is immense. For the full potential to be realized, though, privacy and confidentiality will have to be dealt with carefully. Problematically, many conventional approaches to such pivotal matters as consent, identifiability, and safeguarding and security are inadequate. In many places, research is impeded by an overgrown thicket of laws, regulations, guidance and governance. The challenges are being heightened by the increasing use of biospecimens, and by the globalization of research in a world that has not globalized privacy protection. Drawing on examples from many developed countries and legal jurisdictions, the book critiques the issues, summarizes various ethics, policy, and legal positions (and revisions underway), describes innovative solutions, provides extensive references and suggests ways forward.},
