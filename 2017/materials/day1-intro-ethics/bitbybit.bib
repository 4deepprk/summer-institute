@article{rosenbaum_how_2015,
	title = {How to {See} {More} in {Observational} {Studies}: {Some} {New} {Quasi}-{Experimental} {Devices}},
	volume = {2},
	shorttitle = {How to {See} {More} in {Observational} {Studies}},
	url = {http://dx.doi.org/10.1146/annurev-statistics-010814-020201},
	doi = {10.1146/annurev-statistics-010814-020201},
	abstract = {In a well-conducted, slightly idealized, randomized experiment, the only explanation of an association between treatment and outcome is an effect caused by the treatment. However, this is not true in observational studies of treatment effects, in which treatment and outcomes may be associated because of some bias in the assignment of treatments to individuals. When added to the design of an observational study, quasi-experimental devices investigate empirically a particular rival explanation or counterclaim, often attempting to preempt anticipated counterclaims. This review has three parts: a discussion of the often misunderstood logic of quasi-experimental devices; a brief overview of the important work of Donald T. Campbell and his colleagues (excellent expositions of this work have been published elsewhere); and its main topic, descriptions and empirical examples of newer devices, including evidence factors, differential effects, and the computerized construction of quasi-experiments.},
	number = {1},
	urldate = {2015-11-06},
	journal = {Annual Review of Statistics and Its Application},
	author = {Rosenbaum, Paul R.},
	year = {2015},
	keywords = {differential effects, evidence factors, multiple control groups, sensitivity analysis, strengthening an instrumental variable},
	pages = {21--48},
	file = {Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/W8AVAMAZ/Rosenbaum - 2015 - How to See More in Observational Studies Some New.pdf:application/pdf}
}

