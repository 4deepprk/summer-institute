	issn = {2372-7322, 2372-7330},
	url = {http://bbs.sagepub.com/content/2/1/211},
	doi = {10.1177/2372732215601112},
	abstract = {Many of the official statistics and leading indicators that inform policy decisions are created from aggregating data collected in scientific survey interviews. What happens in the back-and-forth of those interviews{\textemdash}whether a sampled member of the public agrees to participate or not, whether a respondent comprehends questions in the way they were intended or not, whether the interview is spoken or texted{\textemdash}can thus have far-reaching consequences. But the landscape for social measurement is rapidly changing: Participation rates are declining, and people{\textquoteright}s daily communication patterns are evolving with new technologies (text messaging, video chatting, social media posting, etc.). New analyses of survey interactions are demonstrating aspects of interviewer speech that can substantially affect survey participation, which is vital if social measurement is to be trustworthy. Findings also suggest that, once a survey interview starts, the risks of misunderstanding and miscommunication are greater than one might expect, potentially jeopardizing the accuracy of survey results; different approaches to interviewing that allow clarification dialogue can improve respondents{\textquoteright} comprehension and thus survey data quality. Analyses of text messaging and voice interviews on smartphones demonstrate the importance of adapting scientific social measurement to new patterns of communication, adding ways for people to contribute their data at a time and in a mode that is convenient for them even when they are mobile or multitasking.},
	language = {en},
	number = {1},
	urldate = {2015-11-10},
	journal = {Policy Insights from the Behavioral and Brain Sciences},
	author = {Schober, Michael F. and Conrad, Frederick G.},
	month = oct,
	year = {2015},
	keywords = {clarification, data quality, interaction, misunderstanding, new communication technologies, social measurement, survey interview, survey participation},
	pages = {211--219},
	file = {Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/TA7CSQHV/Schober and Conrad - 2015 - Improving Social Measurement by Understanding Inte.pdf:application/pdf;Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/WEIMG3HV/211.html:text/html}
}

