	volume = {38},
	issn = {0190-9320, 1573-6687},
	shorttitle = {When {Treatments} are {Tweets}},
	url = {http://link.springer.com/article/10.1007/s11109-015-9308-6},
	doi = {10.1007/s11109-015-9308-6},
	abstract = {This study rigorously compares the effectiveness of online mobilization appeals via two randomized field experiments conducted over the social microblogging service Twitter. In the process, we demonstrate a methodological innovation designed to capture social effects by exogenously inducing network behavior. In both experiments, we find that direct, private messages to followers of a nonprofit advocacy organization{\textquoteright}s Twitter account are highly effective at increasing support for an online petition. Surprisingly, public tweets have no effect at all. We additionally randomize the private messages to prime subjects with either a {\textquotedblleft}follower{\textquotedblright} or an {\textquotedblleft}organizer{\textquotedblright} identity but find no evidence that this affects the likelihood of signing the petition. Finally, in the second experiment, followers of subjects induced to tweet a link to the petition are more likely to sign it{\textemdash}evidence of a campaign gone {\textquotedblleft}viral.{\textquotedblright} In presenting these results, we contribute to a nascent body of experimental literature exploring political behavior in online social media.},
	language = {en},
	number = {1},
	urldate = {2016-05-31},
	journal = {Political Behavior},
	author = {Coppock, Alexander and Guess, Andrew and Ternovski, John},
	month = mar,
	year = {2016},
	keywords = {Field Experiments, networks, political participation, Political Science, Sociology, general},
	pages = {105--128},
	file = {Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/68NTJ3UG/Coppock et al. - 2015 - When Treatments are Tweets A Network Mobilization.pdf:application/pdf;Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/T3G39UPC/10.html:text/html}
}

@article{cihon_biased_2016,
	title = {A {Biased} {Review} of {Biases} in {Twitter} {Studies} on {Political} {Collective} {Action}},
	url = {http://arxiv.org/abs/1605.04774},
	abstract = {In recent years researchers have gravitated to social media platforms, especially Twitter, as fertile ground for empirical analysis of social phenomena. Social media provides researchers access to trace data of interactions and discourse that once went unrecorded in the offline world. Researchers have sought to use these data to explain social phenomena both particular to social media and applicable to the broader social world. This paper offers a minireview of Twitter-based research on political crowd behavior. This literature offers insight into particular social phenomena on Twitter, but often fails to use standardized methods that permit interpretation beyond individual studies. Moreover, the literature fails to ground methodologies and results in social or political theory, divorcing empirical research from the theory needed to interpret it. Rather, papers focus primarily on methodological innovations for social media analyses, but these too often fail to sufficiently demonstrate the validity of such methodologies. This minireview considers a small number of selected papers; we analyze their (often lack of) theoretical approaches, review their methodological innovations, and offer suggestions as to the relevance of their results for political scientists and sociologists.},
	urldate = {2016-05-31},
	journal = {arXiv:1605.04774 [physics]},
	author = {Cihon, Peter and Yasseri, Taha},
	month = may,
	year = {2016},
