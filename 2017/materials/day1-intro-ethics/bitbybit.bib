@article{reuter_dietary_2008,
	title = {Dietary {Planning} as a {Mediator} of the {Intention}{\textendash}{Behavior} {Relation}: {An} {Experimental}-{Causal}-{Chain} {Design}},
	volume = {57},
	copyright = {{\textcopyright} 2008 The Authors. Journal compilation {\textcopyright} 2008 International Association of Applied Psychology},
	issn = {1464-0597},
	shorttitle = {Dietary {Planning} as a {Mediator} of the {Intention}{\textendash}{Behavior} {Relation}},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1464-0597.2008.00364.x/abstract},
	doi = {10.1111/j.1464-0597.2008.00364.x},
	abstract = {Health behavior change is seen as a self-regulatory process that consists of a motivation phase of goal setting and a volition phase of goal pursuit. Previous studies suggest that the intention{\textendash}behavior association is mediated by planning. However, evidence is based on observational studies rather than on experimental designs. To validate the causal assumptions, an experimental-causal-chain design was employed. Study 1 (n= 145) examined whether changing dietary intentions by a motivational intervention engenders changes in planning activities 1 month later. Study 2 (n= 115) examined, in a different sample, whether a volitional planning intervention engenders changes in dietary behavior 1 month later. In both studies, repeated measures ANOVAs revealed a significant Time {\texttimes} Condition interaction. Changes in intention mediated the effects of the motivational intervention on planning activities (Study 1). Changes in planning mediated the effect of a planning intervention on dietary behavior (Study 2). Previous observational findings on planning as a mediator in the intention{\textendash}behavior association were supported by the two experiments. The findings might help to identify points of intervention in the process underlying health behavior change.Le changement de comportement au plan de la sant{\'e} est consid{\'e}r{\'e} comme un processus d{\textquoteright}auto-r{\'e}gulation qui se compose d{\textquoteright}une phase de motivation (fixation des buts) et d{\textquoteright}une phase de volition (poursuite des buts). Des {\'e}tudes pr{\'e}c{\'e}dentes sugg{\`e}rent que le passage de l{\textquoteright}intention au comportement est m{\'e}diatis{\'e} par la planification. Ces r{\'e}sultats sont obtenus par voie d{\textquoteright}observations plut{\^o}t que par exp{\'e}rimentations. Pour valider les hypoth{\`e}ses de causalit{\'e}, un mod{\`e}le de cha{\^i}ne-causale-exp{\'e}rimentale a {\'e}t{\'e} employ{\'e}. L{\textquoteright}{\'e}tude 1 examine si, en changeant les intentions alimentaires par une intervention motivationnelle, on obtient des changements dans la planification des activit{\'e}s un mois apr{\`e}s (n= 145). L{\textquoteright}{\'e}tude 2 examine, sur un {\'e}chantillon diff{\'e}rent, si la planification d{\textquoteright}une intervention engendre des changements du comportement alimentaire un mois plus tard (n= 115). Pour ces deux {\'e}tudes, des ANOVA {\`a} mesures r{\'e}p{\'e}t{\'e}es r{\'e}v{\`e}lent une interaction significative entre le temps et la condition {\'e}tudi{\'e}e. Les changements de l{\textquoteright}intention influencent les effets de l{\textquoteright}intervention motivationnelle sur la planification des activit{\'e}s ({\'e}tude 1). Les changements de la planification influencent les effets de l{\textquoteright}intervention de la volition sur le comportement alimentaire (Etude 2). Les r{\'e}sultats obtenus par observation sur la planification comme lien entre l{\textquoteright}intention et le comportement sont confirm{\'e}s par les exp{\'e}rimentations. Les r{\'e}sultats peuvent permettre d{\textquoteright}identifier les modalit{\'e}s d{\textquoteright}interventions relatives au processus sous-tendant le changement du comportement de sant{\'e}.},
	language = {en},
	urldate = {2014-01-27},
	journal = {Applied Psychology},
	author = {Reuter, Tabea and Ziegelmann, Jochen P. and Wiedemann, Amelie U. and Lippke, Sonia},
