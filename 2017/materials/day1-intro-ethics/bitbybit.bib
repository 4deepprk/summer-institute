@article{fong_targeted_2012,
	title = {Targeted {Marketing} and {Customer} {Search}},
	url = {http://papers.ssrn.com/sol3/papers.cfm?abstract_id=2097495},
	abstract = {It has become common practice for retailers to personalize direct marketing offers based on customer transaction histories. Targeted email offers featuring products similar to a customer{\textquoteright}s previous purchases generate higher response rates, but also have the potential to affect customer search behavior. A closely matched offer may encourage a customer to start the search process, leading to increased search activity. Alternatively, providing customers with closely matched offers may weaken their incentives to search beyond the targeted items. In a field experiment using email offers sent by an online wine retailer, targeted offers result in less search activity on the retailer{\textquoteright}s website. In a second study, transaction data from an online ticket exchange shows that, after receiving targeted offers, customers are less likely to broaden their purchasing to new genres. These findings indicate that targeted offers carry a hidden cost: a decrease in customer exploration and discovery.},
	urldate = {2012-09-01},
	journal = {SSRN eLibrary},
	author = {Fong, Nathan M.},
