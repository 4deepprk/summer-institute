	file = {Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/XNCQ9268/Marshall et al. - 2015 - Ideas for Citizen Science in Astronomy.pdf:application/pdf}
