	file = {Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/P4993FME/Yeager et al. - 2011 - Comparing the Accuracy of RDD Telephone Surveys an.pdf:application/pdf;Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/H3RE5TAZ/709.html:text/html}
}

@article{huberty_awaiting_2015,
	title = {Awaiting the {Second} {Big} {Data} {Revolution}: {From} {Digital} {Noise} to {Value} {Creation}},
	volume = {15},
	issn = {1566-1679, 1573-7012},
	shorttitle = {Awaiting the {Second} {Big} {Data} {Revolution}},
	url = {http://link.springer.com/article/10.1007/s10842-014-0190-4},
	doi = {10.1007/s10842-014-0190-4},
	abstract = {{\textquotedblleft}Big data{\textquotedblright}{\textemdash}the collection of vast quantities of data about individual behavior via online, mobile, and other data-driven services{\textemdash}has been heralded as the agent of a third industrial revolution{\textemdash}one with raw materials measured in bits, rather than tons of steel or barrels of oil. Yet the industrial revolution transformed not just how firms made things, but the fundamental approach to value creation in industrial economies. To date, big data has not achieved this distinction. Instead, today{\textquoteright}s successful big data business models largely use data to scale old modes of value creation, rather than invent new ones altogether. Moreover, today{\textquoteright}s big data cannot deliver the promised revolution. In this way, today{\textquoteright}s big data landscape resembles the early phases of the first industrial revolution, rather than the culmination of the second a century later. Realizing the second big data revolution will require fundamentally different kinds of data, different innovations, and different business models than those seen to date. That fact has profound consequences for the kinds of investments and innovations firms must seek, and the economic, political, and social consequences that those innovations portend.},
	language = {en},
	number = {1},
	urldate = {2015-07-08},
	journal = {Journal of Industry, Competition and Trade},
	author = {Huberty, Mark},
	month = feb,
	year = {2015},
	keywords = {Big Data, Business models, C80, Digitalization, Economic Policy, European Integration, Industrial Organization, International Economics, L86, M15, Microeconomics, O33, R \& D/Technology Policy, Technological change, Value creation},
	pages = {35--47},
	file = {Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/GQIEI5F7/Huberty - 2015 - Awaiting the Second Big Data Revolution From Digi.pdf:application/pdf;Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/VHHK8KJD/s10842-014-0190-4.html:text/html}
