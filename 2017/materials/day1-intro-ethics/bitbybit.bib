@article{lee_rna_2014,
	title = {{RNA} design rules from a massive open laboratory},
	volume = {111},
	issn = {0027-8424, 1091-6490},
	url = {http://www.pnas.org/content/111/6/2122},
	doi = {10.1073/pnas.1313039111},
	abstract = {Self-assembling RNA molecules present compelling substrates for the rational interrogation and control of living systems. However, imperfect in silico models{\textemdash}even at the secondary structure level{\textemdash}hinder the design of new RNAs that function properly when synthesized. Here, we present a unique and potentially general approach to such empirical problems: the Massive Open Laboratory. The EteRNA project connects 37,000 enthusiasts to RNA design puzzles through an online interface. Uniquely, EteRNA participants not only manipulate simulated molecules but also control a remote experimental pipeline for high-throughput RNA synthesis and structure mapping. We show herein that the EteRNA community leveraged dozens of cycles of continuous wet laboratory feedback to learn strategies for solving in vitro RNA design problems on which automated methods fail. The top strategies{\textemdash}including several previously unrecognized negative design rules{\textemdash}were distilled by machine learning into an algorithm, EteRNABot. Over a rigorous 1-y testing phase, both the EteRNA community and EteRNABot significantly outperformed prior algorithms in a dozen RNA secondary structure design tests, including the creation of dendrimer-like structures and scaffolds for small molecule sensors. These results show that an online community can carry out large-scale experiments, hypothesis generation, and algorithm design to create practical advances in empirical science.},
	language = {en},
	number = {6},
	urldate = {2015-12-30},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Lee, Jeehyung and Kladwang, Wipapat and Lee, Minjae and Cantu, Daniel and Azizyan, Martin and Kim, Hanjoo and Limpaecher, Alex and Gaikwad, Snehal and Yoon, Sungroh and Treuille, Adrien and Das, Rhiju and Participants, EteRNA},
	month = feb,
	year = {2014},
	pmid = {24469816},
	keywords = {Citizen science, crowdsourcing, high-throughput experiments, RNA folding},
	pages = {2122--2127},
	file = {Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/2QMMA8BK/Lee et al. - 2014 - RNA design rules from a massive open laboratory.pdf:application/pdf;Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/BWFZSP2P/2122.html:text/html}
}

@article{sauermann_crowd_2015,
	title = {Crowd science user contribution patterns and their implications},
	volume = {112},
	issn = {0027-8424, 1091-6490},
	url = {http://www.pnas.org/content/112/3/679},
	doi = {10.1073/pnas.1408907112},
	abstract = {Scientific research performed with the involvement of the broader public (the crowd) attracts increasing attention from scientists and policy makers. A key premise is that project organizers may be able to draw on underused human resources to advance research at relatively low cost. Despite a growing number of examples, systematic research on the effort contributions volunteers are willing to make to crowd science projects is lacking. Analyzing data on seven different projects, we quantify the financial value volunteers can bring by comparing their unpaid contributions with counterfactual costs in traditional or online labor markets. The volume of total contributions is substantial, although some projects are much more successful in attracting effort than others. Moreover, contributions received by projects are very uneven across time{\textemdash}a tendency toward declining activity is interrupted by spikes typically resulting from outreach efforts or media attention. Analyzing user-level data, we find that most contributors participate only once and with little effort, leaving a relatively small share of users who return responsible for most of the work. Although top contributor status is earned primarily through higher levels of effort, top contributors also tend to work faster. This speed advantage develops over multiple sessions, suggesting that it reflects learning rather than inherent differences in skills. Our findings inform recent discussions about potential benefits from crowd science, suggest that involving the crowd may be more effective for some kinds of projects than others, provide guidance for project managers, and raise important questions for future research.},
	language = {en},
	number = {3},
	urldate = {2015-12-30},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Sauermann, Henry and Franzoni, Chiara},
	month = jan,
	year = {2015},
	pmid = {25561529},
	keywords = {Citizen science, Crowd science, crowdsourcing, dynamics, effort valuation},
	pages = {679--684},
	file = {Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/SN8WJMNI/Sauermann and Franzoni - 2015 - Crowd science user contribution patterns and their.pdf:application/pdf;Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/A3KCHR87/679.html:text/html}
}

