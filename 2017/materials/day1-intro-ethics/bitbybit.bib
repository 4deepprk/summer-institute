@article{rothstein_does_2013,
	title = {Does consent bias research?},
	volume = {13},
	issn = {1536-0075},
	doi = {10.1080/15265161.2013.767955},
	abstract = {Researchers increasingly rely on large data sets of health information, often linked with biological specimens. In recent years, the argument has been made that obtaining informed consent for conducting records-based research is unduly burdensome and results in "consent bias." As a type of selection bias, consent bias is said to exist when the group giving researchers access to their data differs from the group denying access. Therefore, to promote socially beneficial research, it is argued that consent should be unnecessary. After analyzing the biostatistics evidence and bioethics arguments, the article concludes that (1) claims about the amount of consent bias are overstated; (2) commonly used statistical methods usually can account for consent bias; and (3) any residual effects of consent bias are below an acceptable level of imprecision and constitute a reasonable social cost for conducting ethically responsible research.},
	language = {eng},
	number = {4},
	journal = {The American journal of bioethics: AJOB},
	author = {Rothstein, Mark A. and Shoben, Abigail B.},
	year = {2013},
	pmid = {23514393},
	keywords = {Biological Specimen Banks, Biomedical Research, Blood Donors, Ethical Theory, Ethics, Research, Exercise, Humans, Informed consent, Institute of Medicine (U.S.), Odds Ratio, Patient Selection, Personal Autonomy, public policy, research design, Research Subjects, selection bias, Social Values, United States},
	pages = {27--37}
}

