@techreport{blattman_measuring_2014,
	address = {Rochester, NY},
	type = {{SSRN} {Scholarly} {Paper}},
	title = {Measuring the {Measurement} {Error}: {A} {Method} to {Qualitatively} {Validate} {Survey} {Data}},
	shorttitle = {Measuring the {Measurement} {Error}},
	url = {http://papers.ssrn.com/abstract=2449048},
	abstract = {Field experiments rely heavily on self-reported data, but subjects may misreport behaviors, especially sensitive ones such as crime or drug use. Concerns that treatment affects outcome reporting undermine the credibility of experimental estimates. We develop a data validation technique that uses intensive qualitative work to assess measurement error in random subsamples. Subjects were enrolled in experimental programs to reduce poverty and crime. Trained local researchers spent several days engaging subjects within days of their endline surveys, validating the incidence of theft, drug use, homelessness, gambling, and two expenditures. Underreporting is less common than expected, but control group members were more likely to underreport sensitive behaviors and expenditures, probably from strategic behavior. We bound survey-based treatment effects estimates accordingly. Actual measurement error patterns were the opposite of expected, suggesting the usefulness of this technique for improving our understanding of survey misreporting and increasing the credibility of self-reported outcomes in field experiments.},
