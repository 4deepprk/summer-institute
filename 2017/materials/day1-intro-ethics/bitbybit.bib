	author = {Grusky, David B. and Smeeding, Timothy M. and Snipp, C. Matthew},
	month = jan,
	year = {2015},
	keywords = {administrative data, economic mobility, occupational mobility, panel data, social mobility},
	pages = {63--82},
	file = {Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/BMMD2XCN/Grusky et al. - 2015 - A New Infrastructure for Monitoring Social Mobilit.pdf:application/pdf;Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/6JVVTCJZ/63.html:text/html}
}

