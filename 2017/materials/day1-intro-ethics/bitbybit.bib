	keywords = {Ifeoma  Ajunwa, Jason  Schultz, Kate  Crawford, Limitless Worker Surveillance, SSRN},
	file = {Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/4KRM7433/papers.html:text/html}
}

