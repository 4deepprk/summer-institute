	pages = {486--488},
	file = {Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/WED7X2PB/Hofman et al. - 2017 - Prediction and explanation in social systems.pdf:application/pdf;Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/ND6KWUDR/486.html:text/html}
}

