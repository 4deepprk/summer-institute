	year = {2015},
	keywords = {Atmospheric Sciences, Biodiversity, Digital conservation, ecology, Environmental Engineering/Biotechnology, Environmental Management, Environment, general, Information and Communication Technology (ICT), innovation, Nature conservation, Physical Geography, The Information Age},
	pages = {661--673},
	file = {Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/WG72K82F/Arts et al. - 2015 - Digital technology and the conservation of nature.pdf:application/pdf;Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/N5EKN6SN/s13280-015-0705-1.html:text/html}
