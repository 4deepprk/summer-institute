@article{salganik_leading_2008,
	title = {Leading the {Herd} {Astray}: {An} {Experimental} {Study} of {Self}-fulfilling {Prophecies} in an {Artificial} {Cultural} {Market}},
	volume = {71},
	shorttitle = {Leading the {Herd} {Astray}},
	url = {http://spq.sagepub.com/content/71/4/338.abstract},
	doi = {10.1177/019027250807100404},
	abstract = {Individuals influence each others' decisions about cultural products such as songs, books, and movies; but to what extent can the perception of success become a {\textquotedblleft}self-fulfilling prophecy{\textquotedblright}? We have explored this question experimentally by artificially inverting the true popularity of songs in an online {\textquotedblleft}music market,{\textquotedblright} in which 12,207 participants listened to and downloaded songs by unknown bands. We found that most songs experienced self-ful- filling prophecies, in which perceived{\textemdash}but initially false{\textemdash}popularity became real over time. We also found, however, that the inversion was not self-fulfilling for the market as a whole, in part because the very best songs recovered their popularity in the long run. Moreover, the distortion of market information reduced the correlation between appeal and popularity, and led to fewer overall downloads. These results, although partial and speculative, suggest a new approach to the study of cultural markets, and indicate the potential of web-based experiments to explore the social psychological origin of other macrosocio- logical phenomena.},
	number = {4},
	urldate = {2011-05-01},
	journal = {Social Psychology Quarterly},
	author = {Salganik, Matthew J. and Watts, Duncan J.},
	month = dec,
	year = {2008},
	pages = {338--355},
