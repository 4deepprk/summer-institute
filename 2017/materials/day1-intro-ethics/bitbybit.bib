	file = {JSTOR Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/AMAKWQ93/Freedman - 1991 - Statistical Models and Shoe Leather.pdf:application/pdf}
}

