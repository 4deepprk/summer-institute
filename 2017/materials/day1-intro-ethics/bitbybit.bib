@article{abraham_nonresponse_2006,
	title = {Nonresponse in the {American} {Time} {Use} {Survey} {Who} {Is} {Missing} from the {Data} and {How} {Much} {Does} {It} {Matter}?},
	volume = {70},
	issn = {0033-362X, 1537-5331},
	url = {http://poq.oxfordjournals.org/content/70/5/676},
	doi = {10.1093/poq/nfl037},
	abstract = {This article examines nonresponse in a large government survey, the American Time Use Survey (ATUS), which interviews persons in households previously interviewed in the Current Population Survey. The response rate for the ATUS has been below 60 percent for the first two years of its existence, raising questions about whether the results can be generalized to the target population. The article begins with an analysis of the types of nonresponse encountered in the ATUS. Noncontact accounts for roughly 60 percent of ATUS nonresponse, with refusals accounting for roughly 40 percent. We find little support for the hypothesis that busy people are less likely to respond to the ATUS but find considerable support for the hypothesis that people who are weakly integrated into their communities are less likely to respond, mostly because they are less likely to be contacted. When we compare aggregate estimates of time use calculated using the ATUS base weights without any adjustment for nonresponse, estimates calculated using the ATUS final weights with a nonresponse adjustment, and estimates calculated using weights that incorporate our own nonresponse adjustment based on a propensity model, we find some modest differences, but the three sets of estimates are broadly similar. The article ends with suggestions for further research and analysis.},
	language = {en},
	number = {5},
	urldate = {2015-09-02},
	journal = {Public Opinion Quarterly},
	author = {Abraham, Katharine G. and Maitland, Aaron and Bianchi, Suzanne M.},
	month = jan,
	year = {2006},
	pages = {676--703},
	file = {Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/W4GQIZ3B/Abraham et al. - 2006 - Nonresponse in the American Time Use Survey Who Is.pdf:application/pdf;Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/E4MDV2BD/Abraham et al. - 2006 - Nonresponse in the American Time Use Survey Who Is.pdf:application/pdf;Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/ASQZ7HA4/676.html:text/html;Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/UVBZ8P5G/676.html:text/html}
