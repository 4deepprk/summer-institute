@article{ling-jyh_chen_design_2010,
	title = {The {Design} of {Puzzle} {Selection} {Strategies} for {ESP}-{Like} {GWAP} {Systems}},
	volume = {2},
	issn = {1943-068X},
	doi = {10.1109/TCIAIG.2010.2049846},
	abstract = {The {\textquotedblleft}games with a purpose{\textquotedblright} (GWAP) genre is a type of {\textquotedblleft}human computation{\textquotedblright} that outsources certain steps of the computational process to humans. Although most GWAP studies focus on the design and analysis of GWAP systems, a systematic and thorough evaluation of existing systems is lacking. We address the issue in this paper. Taking the ESP game as an example, we propose a metric, called system utility, for evaluating the performance of GWAP systems, and use analysis to study the properties of the ESP game. We argue that GWAP systems should be designed and played with strategies. To this end, based on our analysis, we implement an optimal puzzle selection strategy (OPSA) to improve GWAP systems. Using a comprehensive set of simulations, we show that the proposed OPSA approach can improve the system utility of the ESP game significantly. In addition, we implement a quasi ESP game, called ESP Lite, which embeds three puzzle selection algorithms transparently and records the complete game trace for evaluation and further research. During a one-month experiment, we have investigated the inner properties of the three strategies in real-world GWAP systems, and verified that the OPSA scheme achieves the best system utility for the ESP game. The results of this study demonstrate that GWAP systems are more efficient if they are designed and played with strategies.},
	language = {English},
	number = {2},
	journal = {IEEE Transactions on Computational Intelligence and AI in Games},
	author = {Ling-Jyh Chen and Bo-Chun Wang and Wen-Yuan Zhu},
	month = jun,
	year = {2010},
	keywords = {computer games, esp game, ESP like GWAP systems, ESP Lite, games with a purpose, Games with a purpose (GWAP), human computation, human computer interaction, optimal puzzle selection strategy, performance evaluation, puzzle selection strategy design, system utility metric, tagging},
