@techreport{agarwal_banks_2015,
	type = {Working {Paper}},
	title = {Do {Banks} {Pass} {Through} {Credit} {Expansions}? {The} {Marginal} {Profitability} of {Consumer} {Lending} {During} the {Great} {Recession}},
	shorttitle = {Do {Banks} {Pass} {Through} {Credit} {Expansions}?},
	url = {http://www.nber.org/papers/w21567},
	abstract = {We examine the ability of policymakers to stimulate household borrowing and spending during the Great Recession by reducing banks{\textquoteright} cost of funds. Using panel data on 8.5 million U.S. credit card accounts and 743 credit limit regression discontinuities, we estimate the marginal propensity to borrow (MPB) for households with different FICO credit scores. We find substantial heterogeneity, with a \$1 increase in credit limits raising total unsecured borrowing after 12 months by 59 cents for consumers with the lowest FICO scores (<= 660) while having no effect on consumers with the highest FICO scores ({\textgreater} 740). We use the same credit limit regression discontinuities to estimate banks{\textquoteright} marginal propensity to lend (MPL) out of a decrease in their cost of funds. For the lowest FICO score households, higher credit limits quickly reduce marginal profits, limiting the pass-through of credit expansions to those households. We estimate that a 1 percentage point reduction in the cost of funds raises optimal credit limits by \$127 for consumers with FICO scores below 660 versus \$2,203 for consumers with FICO scores above 740. We conclude that banks{\textquoteright} MPL is lowest exactly for those households with the highest MPB, limiting the effectiveness of policies that aim to stimulate the economy by reducing banks{\textquoteright} cost of funds.},
	number = {21567},
	urldate = {2015-12-22},
	institution = {National Bureau of Economic Research},
	author = {Agarwal, Sumit and Chomsisengphet, Souphala and Mahoney, Neale and Stroebel, Johannes},
	month = sep,
	year = {2015},
	file = {NBER Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/NKPJ8H4H/Agarwal et al. - 2015 - Do Banks Pass Through Credit Expansions The Margi.pdf:application/pdf}
