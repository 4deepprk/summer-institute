@article{courter_weekend_2012,
	title = {Weekend bias in {Citizen} {Science} data reporting: implications for phenology studies},
	volume = {57},
	issn = {0020-7128, 1432-1254},
	shorttitle = {Weekend bias in {Citizen} {Science} data reporting},
	url = {http://link.springer.com/article/10.1007/s00484-012-0598-7},
	doi = {10.1007/s00484-012-0598-7},
	abstract = {Studies of bird phenology can help elucidate the effects of climate change on wildlife species but observations over broad spatial scales are difficult without a network of observers. Recently, networks of citizen volunteers have begun to report first arrival dates for many migratory species. Potential benefits are substantial (e.g., understanding ecological processes at broad spatial and temporal scales) if known biases of citizen data reporting are identified and addressed. One potential source of bias in bird phenology studies is the tendency for more {\textquotedblleft}first{\textquotedblright} migratory arrivals to be reported on weekends than on weekdays. We investigated weekend bias in data reporting for five common bird species in North America (Baltimore Oriole, Icterus galbula; Barn Swallow, Hirundo rustica; Chimney Swift, Chaetura pelagica; Purple Martin, Progne subis; and Ruby-throated Hummingbird, Archilochus colubris), and assessed whether this bias affected mean arrival dates reported using data from historical (1880{\textendash}1969; N = 25,555) and recent (1997{\textendash}2010; N = 63,149) Citizen Science databases. We found a greater percentage of first arrivals reported on weekends and small but significant differences in mean arrival dates (approximately 0.5 days) for four of five species. Comparing time periods, this weekend bias decreased from 33.7 \% and five species in the historical time period to 32 \% and three species in the recent, perhaps related to changes in human activity patterns. Our results indicate that weekend bias in citizen data reporting is decreasing over time in North America and including a {\textquoteleft}day of week{\textquoteright} term in models examining changes in phenology could help make conclusions more robust.},
	language = {en},
	number = {5},
	urldate = {2016-01-22},
	journal = {International Journal of Biometeorology},
	author = {Courter, Jason R. and Johnson, Ron J. and Stuyck, Claire M. and Lang, Brian A. and Kaiser, Evan W.},
	month = oct,
	year = {2012},
	keywords = {Animal Physiology, Biophysics and Biological Physics, Bird phenology, Climate change, Detectability, Environmental Health, Environment, general, First arrival dates, Meteorology, Plant Physiology, Spring migration, volunteers},
	pages = {715--720},
	file = {Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/R7HEKMKZ/Courter et al. - 2012 - Weekend bias in Citizen Science data reporting im.pdf:application/pdf;Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/ZVSQQIND/10.html:text/html}
}

@article{watkins_hearsay_2009,
	title = {Hearsay {Ethnography}: {Conversational} {Journals} as a {Method} for {Studying} {Culture} in {Action}},
	volume = {37},
	issn = {0304-422X},
	shorttitle = {Hearsay {Ethnography}},
	url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC2791322/},
	doi = {10.1016/j.poetic.2009.03.002},
	abstract = {Social scientists have long struggled to develop methods adequate to their theoretical understanding of meaning as collective and dynamic. While culture is widely understood as an emergent property of collectivities, the methods we use keep pulling us back towards interview-situated accounts and an image of culture as located in individual experience. Scholars who seek to access supra-individual semiotic structures by studying public rituals and other collectively-produced texts then have difficulty capturing the dynamic processes through which such meanings are created and changed in situ. To try to capture more effectively the way meaning is produced and re-produced in everyday life, we focus here on conversational interactions{\textemdash}the voices and actions that constitute the relational space among actors. Conversational journals provide us with a method: the analysis of texts produced by cultural insiders who keep journals of who-said-what-to-whom in conversations they overhear or events they participate in during the course of their daily lives. We describe the method, distinguishing it from other approaches and noting its drawbacks. We then illustrate the methodological advantages of conversational journals with examples from our texts. We end with a discussion of the method{\textquoteright}s potential in our setting as well as in other places and times.},
	number = {2},
	urldate = {2016-01-22},
	journal = {Poetics},
	author = {Watkins, Susan Cotts and Swidler, Ann},
	month = apr,
	year = {2009},
	pmid = {20161457},
	pmcid = {PMC2791322},
	pages = {162--184},
	file = {PubMed Central Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/PNJTZWS5/Watkins and Swidler - 2009 - Hearsay Ethnography Conversational Journals as a .pdf:application/pdf}
}

