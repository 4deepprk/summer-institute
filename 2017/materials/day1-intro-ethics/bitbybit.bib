@article{blumenstock_risk_2011,
	title = {Risk and {Reciprocity} {Over} the {Mobile} {Phone} {Network}: {Evidence} from {Rwanda}},
	shorttitle = {Risk and {Reciprocity} {Over} the {Mobile} {Phone} {Network}},
	url = {http://papers.ssrn.com/sol3/papers.cfm?abstract_id=1958042},
	abstract = {A large literature describes how local risk sharing networks can help individuals smooth consumption in the face of idiosyncratic economic shocks. However, when an entire community faces a large covariate shock, and when the transaction costs of transfers are high, these risk sharing networks are likely to be less effective. In this paper, we document how a new technology {\textendash} mobile phones {\textendash} reduces transaction costs and enables Rwandans to share risk quickly over long distances. We examine a comprehensive database of person-to-person transfers of mobile airtime and find that individuals send this rudimentary form of {\textquotedblleft}mobile money{\textquotedblright} to friends and family affected by natural disasters. Using the Lake Kivu earthquake of 2008 to identify the effect of a large covariate shock on interpersonal transfers, we estimate that a current-day earthquake would result in the transfer of between \$22,000 and \$30,000 to individuals living near the epicenter. We further show that the pattern of transfers is most consistent with a model of reciprocal risk sharing, where transfers are determined by past reciprocity and geographical proximity, rather than one of pure charity or altruism, in which transfers would be expected to be increasing in the wealth of the sender and decreasing in the wealth of the recipient.},
	urldate = {2012-10-29},
	journal = {SSRN eLibrary},
	author = {Blumenstock, Joshua Evan and Fafchamps, Marcel and Eagle, Nathan},
	month = oct,
	year = {2011},
	keywords = {Africa, Development, Earthquakes, Information and communications technologies, Mobile Money, mobile phones, Risk Sharing, Rwanda},
	file = {SSRN Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/AFM3D4C4/papers.html:text/html}
}

@article{gerber_social_2008,
