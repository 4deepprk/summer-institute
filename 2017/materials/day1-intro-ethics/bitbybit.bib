	file = {PLoS Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/JZRMGNHT/Toole et al. - 2012 - Modeling the Adoption of Innovations in the Presen.pdf:application/pdf}
