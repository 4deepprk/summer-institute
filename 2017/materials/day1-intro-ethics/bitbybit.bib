@article{boyd_critical_2012,
	title = {Critical {Questions} for {Big} {Data}},
	volume = {15},
	issn = {1369-118X},
	url = {http://www.tandfonline.com/doi/abs/10.1080/1369118X.2012.678878},
	doi = {10.1080/1369118X.2012.678878},
	abstract = {The era of Big Data has begun. Computer scientists, physicists, economists, mathematicians, political scientists, bio-informaticists, sociologists, and other scholars are clamoring for access to the massive quantities of information produced by and about people, things, and their interactions. Diverse groups argue about the potential benefits and costs of analyzing genetic sequences, social media interactions, health records, phone logs, government records, and other digital traces left by people. Significant questions emerge. Will large-scale search data help us create better tools, services, and public goods? Or will it usher in a new wave of privacy incursions and invasive marketing? Will data analytics help us understand online communities and political movements? Or will it be used to track protesters and suppress speech? Will it transform how we study human communication and culture, or narrow the palette of research options and alter what {\textquoteleft}research{\textquoteright} means? Given the rise of Big Data as a socio-technical phenomenon, we argue that it is necessary to critically interrogate its assumptions and biases. In this article, we offer six provocations to spark conversations about the issues of Big Data: a cultural, technological, and scholarly phenomenon that rests on the interplay of technology, analysis, and mythology that provokes extensive utopian and dystopian rhetoric.},
	number = {5},
	urldate = {2012-11-04},
	journal = {Information, Communication \& Society},
	author = {boyd, danah and Crawford, Kate},
	year = {2012},
	pages = {662--679},
	file = {Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/QPTAKE78/boyd and Crawford - 2012 - CRITICAL QUESTIONS FOR BIG DATA.pdf:application/pdf;Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/U5ZTHQZ5/1369118X.2012.html:text/html}
