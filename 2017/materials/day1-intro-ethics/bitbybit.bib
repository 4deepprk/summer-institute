	author = {Khan, Muhammad Raza and Blumenstock, Joshua E.},
	month = jun,
	year = {2016},
	note = {arXiv: 1606.05105},
	keywords = {Computer Science - Computers and Society, Statistics - Machine Learning},
	file = {arXiv\:1606.05105 PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/7XXJZGIR/Khan and Blumenstock - 2016 - Machine Learning Across Cultures Modeling the Ado.pdf:application/pdf;arXiv.org Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/DH3JEJVV/1606.html:text/html}
