@book{oneil_weapons_2016,
	address = {New York},
	title = {Weapons of {Math} {Destruction}: {How} {Big} {Data} {Increases} {Inequality} and {Threatens} {Democracy}},
	isbn = {978-0-553-41881-1},
	shorttitle = {Weapons of {Math} {Destruction}},
	abstract = {A former Wall Street quant sounds an alarm on the mathematical models that pervade modern life~{\textemdash} and threaten to rip apart our social fabricWe live in the age of the algorithm. Increasingly, the decisions that affect our lives{\textemdash}where we go to school, whether we get a car loan, how much we pay for health insurance{\textemdash}are being made not by humans, but by mathematical models. In theory, this should lead to greater fairness: Everyone is judged according to the same rules, and bias is eliminated.But as Cathy O{\textquoteright}Neil reveals in this urgent and necessary book, the opposite is true. The models being used today are opaque, unregulated, and uncontestable, even when they{\textquoteright}re wrong. Most troubling, they reinforce discrimination: If a poor student can{\textquoteright}t get a loan because a lending model deems him too risky (by virtue of his zip code), he{\textquoteright}s then cut off from the kind of education that could pull him out of poverty, and a vicious spiral ensues. Models are propping up the lucky and punishing the downtrodden, creating a {\textquotedblleft}toxic cocktail for democracy.{\textquotedblright} Welcome to the dark side of Big Data.Tracing the arc of a person{\textquoteright}s life, O{\textquoteright}Neil exposes the black box models that shape our future, both as individuals and as a society. These {\textquotedblleft}weapons of math destruction{\textquotedblright} score teachers and students, sort r{\'e}sum{\'e}s, grant (or deny) loans, evaluate workers, target voters, set parole, and monitor our health.O{\textquoteright}Neil calls on modelers to take more responsibility for their algorithms and on policy makers to regulate their use. But in the end, it{\textquoteright}s up to us to become more savvy about the models that govern our lives. This important book empowers us to ask the tough questions, uncover the truth, and demand change.},
	language = {English},
	publisher = {Crown},
	author = {O'Neil, Cathy},
	month = sep,
	year = {2016}
}

