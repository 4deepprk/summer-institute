}

@article{mullinix_generalizability_2015,
	title = {The {Generalizability} of {Survey} {Experiments}},
	volume = {2},
	issn = {2052-2649},
	url = {http://journals.cambridge.org/article_S2052263015000196},
	doi = {10.1017/XPS.2015.19},
	abstract = {Survey experiments have become a central methodology across the social sciences. Researchers can combine experiments{\textquoteright} causal power with the generalizability of population-based samples. Yet, due to the expense of population-based samples, much research relies on convenience samples (e.g. students, online opt-in samples). The emergence of affordable, but non-representative online samples has reinvigorated debates about the external validity of experiments. We conduct two studies of how experimental treatment effects obtained from convenience samples compare to effects produced by population samples. In Study 1, we compare effect estimates from four different types of convenience samples and a population-based sample. In Study 2, we analyze treatment effects obtained from 20 experiments implemented on a population-based sample and Amazon's Mechanical Turk (MTurk). The results reveal considerable similarity between many treatment effects obtained from convenience and nationally representative population-based samples. While the results thus bolster confidence in the utility of convenience samples, we conclude with guidance for the use of a multitude of samples for advancing scientific knowledge.},
	number = {02},
	urldate = {2016-03-18},
	journal = {Journal of Experimental Political Science},
	author = {Mullinix, Kevin J. and Leeper, Thomas J. and Druckman, James N. and Freese, Jeremy},
	month = dec,
	year = {2015},
	pages = {109--138},
	file = {Cambridge Journals Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/93HBGDDN/displayAbstract.html:text/html;Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/ATWIURFT/Mullinix et al. - 2015 - The Generalizability of Survey Experiments.pdf:application/pdf}
}

