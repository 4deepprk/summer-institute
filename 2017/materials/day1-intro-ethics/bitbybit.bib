	shorttitle = {Beyond {AMT}},
	url = {http://arxiv.org/abs/1310.1672},
	abstract = {While Amazon's Mechanical Turk (AMT) helped launch the paid crowd work industry eight years ago, many new vendors now offer a range of alternative models. Despite this, little crowd work research has explored other platforms. Such near-exclusive focus risks letting AMT's particular vagaries and limitations overly shape our understanding of crowd work and the research questions and directions being pursued. To address this, we present a cross-platform content analysis of seven crowd work platforms. We begin by reviewing how AMT assumptions and limitations have influenced prior research. Next, we formulate key criteria for characterizing and differentiating crowd work platforms. Our analysis of platforms contrasts them with AMT, informing both methodology of use and directions for future research. Our cross-platform analysis represents the only such study by researchers for researchers, intended to further enrich the diversity of research on crowd work and accelerate progress.},
	urldate = {2016-01-10},
	journal = {arXiv:1310.1672 [cs]},
	author = {Vakharia, Donna and Lease, Matthew},
	month = oct,
	year = {2013},
	note = {arXiv: 1310.1672},
	keywords = {Computer Science - Computers and Society, Computer Science - Human-Computer Interaction},
	file = {arXiv\:1310.1672 PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/PQXXKQ6M/Vakharia and Lease - 2013 - Beyond AMT An Analysis of Crowd Work Platforms.pdf:application/pdf;arXiv.org Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/WS8TZNJ2/1310.html:text/html}
}

@article{beaumont_milky_2014,
	title = {The {Milky} {Way} {Project}: {Leveraging} {Citizen} {Science} and {Machine} {Learning} to {Detect} {Interstellar} {Bubbles}},
	volume = {214},
	issn = {0067-0049},
	shorttitle = {The {Milky} {Way} {Project}},
	url = {http://stacks.iop.org/0067-0049/214/i=1/a=3},
	doi = {10.1088/0067-0049/214/1/3},
	abstract = {We present Brut, an algorithm to identify bubbles in infrared images of the Galactic midplane. Brut is based on the Random Forest algorithm, and uses bubbles identified by {\textgreater}35,000 citizen scientists from the Milky Way Project to discover the identifying characteristics of bubbles in images from the Spitzer Space Telescope . We demonstrate that Brut's ability to identify bubbles is comparable to expert astronomers. We use Brut to re-assess the bubbles in the Milky Way Project catalog, and find that 10\%-30\% of the objects in this catalog are non-bubble interlopers. Relative to these interlopers, high-reliability bubbles are more confined to the mid-plane, and display a stronger excess of young stellar objects along and within bubble rims. Furthermore, Brut is able to discover bubbles missed by previous searches{\textemdash}particularly bubbles near bright sources which have low contrast relative to their surroundings. Brut demonstrates the synergies that exist between citizen scientists, professional scientists, and machine learning techniques. In cases where "untrained" citizens can identify patterns that machines cannot detect without training, machine learning algorithms like Brut can use the output of citizen science projects as input training sets, offering tremendous opportunities to speed the pace of scientific discovery. A hybrid model of machine learning combined with crowdsourced training data from citizen scientists can not only classify large quantities of data, but also address the weakness of each approach if deployed alone.},
	language = {en},
	number = {1},
	urldate = {2016-01-10},
	journal = {The Astrophysical Journal Supplement Series},
	author = {Beaumont, Christopher N. and Goodman, Alyssa A. and Kendrew, Sarah and Williams, Jonathan P. and Simpson, Robert},
	year = {2014},
	pages = {3},
	file = {IOP Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/NPT7PGV6/Beaumont et al. - 2014 - The Milky Way Project Leveraging Citizen Science .pdf:application/pdf}
}

