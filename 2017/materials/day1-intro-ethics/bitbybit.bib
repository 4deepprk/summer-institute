@article{centola_spontaneous_2015,
	title = {The spontaneous emergence of conventions: {An} experimental study of cultural evolution},
	volume = {112},
	issn = {0027-8424, 1091-6490},
	shorttitle = {The spontaneous emergence of conventions},
	url = {http://www.pnas.org/content/112/7/1989},
	doi = {10.1073/pnas.1418838112},
	abstract = {How do shared conventions emerge in complex decentralized social systems? This question engages fields as diverse as linguistics, sociology, and cognitive science. Previous empirical attempts to solve this puzzle all presuppose that formal or informal institutions, such as incentives for global agreement, coordinated leadership, or aggregated information about the population, are needed to facilitate a solution. Evolutionary theories of social conventions, by contrast, hypothesize that such institutions are not necessary in order for social conventions to form. However, empirical tests of this hypothesis have been hindered by the difficulties of evaluating the real-time creation of new collective behaviors in large decentralized populations. Here, we present experimental results{\textemdash}replicated at several scales{\textemdash}that demonstrate the spontaneous creation of universally adopted social conventions and show how simple changes in a population{\textquoteright}s network structure can direct the dynamics of norm formation, driving human populations with no ambition for large scale coordination to rapidly evolve shared social conventions.},
	language = {en},
	number = {7},
	urldate = {2016-04-07},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Centola, Damon and Baronchelli, Andrea},
	month = feb,
	year = {2015},
	pmid = {25646462},
	keywords = {complex systems, empirical testing, network science, social conventions, spontaneous emergence},
	pages = {1989--1994},
	file = {Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/JCSPUJF5/Centola and Baronchelli - 2015 - The spontaneous emergence of conventions An exper.pdf:application/pdf;Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/39IX3C5S/1989.html:text/html}
}

