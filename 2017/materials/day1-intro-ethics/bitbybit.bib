	keywords = {conversation analysis, ethnography, ethnomethodology, interaction order, longitudinal ethnography},
	pages = {210--218},
	file = {Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/5A3TSC6M/Maynard - 2014 - News From Somewhere, News From Nowhere On the Stud.pdf:application/pdf;Snapshot:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/CIMPTKU7/210.html:text/html}
}

