	pages = {1301--1318},
	file = {ACM Full Text PDF:/Users/mjs3/Library/Application Support/Firefox/Profiles/7jaaz9xm.default/zotero/storage/2QD6PUQA/Kittur et al. - 2013 - The Future of Crowd Work.pdf:application/pdf}
